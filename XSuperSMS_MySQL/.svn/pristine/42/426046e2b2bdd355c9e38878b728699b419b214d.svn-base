<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/activity_bg"
    android:orientation="vertical" >

    <include
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_bar_top"
        layout="@layout/layout_header" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="5dp"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <ScrollView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/plan_content_bj" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:padding="2dp" >

                <TextView
                    style="@style/font2_black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/sheet" />

                <TextView
                    android:id="@+id/workinfo_reserve_steel_count"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_green_light"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_steel_weight"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_orange_light"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_steel_count_new"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_new_work"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_steel_weight_new"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_new_work"
                    android:gravity="center" />

                <TextView
                    style="@style/font2_black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/remnant" />

                <TextView
                    android:id="@+id/workinfo_reserve_margin_count"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_green_light"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_margin_weight"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_orange_light"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_margin_count_new"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_new_work"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_margin_weight_new"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_new_work"
                    android:gravity="center" />

                <TextView
                    style="@style/font2_black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/articlesBeingProcessed" />

                <TextView
                    android:id="@+id/workinfo_reserve_making_count"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_green_light"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_making_weight"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_orange_light"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_making_count_new"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_new_work"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/workinfo_reserve_making_weight_new"
                    style="@style/font3_white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/plan_type_new_work"
                    android:gravity="center" />
            </LinearLayout>
        </ScrollView>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_weight="3"
            android:background="@drawable/plan_content_bj"
            android:orientation="vertical"
            android:padding="2dp" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.15" >

                <Button
                    android:id="@+id/workinfo_reserve_steel_btn"
                    style="@style/font2_black"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/workinfo_useratio_btn_selector"
                    android:gravity="center"
                    android:text="@string/sheet" />

                <Button
                    android:id="@+id/workinfo_reserve_margin_btn"
                    style="@style/font2_black"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/workinfo_useratio_btn_selector"
                    android:gravity="center"
                    android:text="@string/remnant" />

                <Button
                    android:id="@+id/workinfo_reserve_making_btn"
                    style="@style/font2_black"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/workinfo_useratio_btn_selector"
                    android:gravity="center"
                    android:text="@string/articlesBeingProcessed" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.7" >

                <LinearLayout
                    android:id="@+id/workinfo_reserve_weight_chart"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/workinfo_reserve_count_chart"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:orientation="vertical" >
                </LinearLayout>
            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <include
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center_vertical"
                    android:layout_marginRight="5dp"
                    layout="@layout/layout_timebtn_group" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="30dp"
                            android:layout_height="1dp"
                            android:layout_gravity="center"
                            android:background="@color/orange" />

                        <TextView
                            android:id="@+id/workinfo_reserve_sheet_weight"
                            style="@style/font3_black"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:text="@string/steel_weight" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="30dp"
                            android:layout_height="1dp"
                            android:layout_gravity="center"
                            android:background="@color/green" />

                        <TextView
                            android:id="@+id/workinfo_reserve_sheet_count"
                            style="@style/font3_black"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:text="@string/steel_count" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

    <include
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_bar_bottom"
        layout="@layout/layout_footer" />

</LinearLayout>