<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/planProHeight"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/prcent_tv_fin"
            style="@style/font3_white"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/worklook_green"
            android:singleLine="true" />

        <TextView
            android:id="@+id/prcent_tv_unf"
            style="@style/font3_white"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/worklook_gray"
            android:singleLine="true" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/percent_type_1_view"
            android:layout_width="@dimen/legendHeight"
            android:layout_height="@dimen/legendHeight"
            android:background="@drawable/worklook_legend_green"
            android:typeface="normal" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/percent_type_1_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@string/workInfo_finish"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize_4" />

            <TextView
                android:id="@+id/percent_type_1_value1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize_4" />
        </LinearLayout>

        <TextView
            android:id="@+id/percent_type_2_view"
            android:layout_width="@dimen/legendHeight"
            android:layout_height="@dimen/legendHeight"
            android:layout_marginLeft="3dp"
            android:background="@drawable/worklook_legend_gray"
            android:typeface="normal" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/percent_type_2_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@string/workInfo_unfinish"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize_4" />

            <TextView
                android:id="@+id/percent_type_2_value1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize_4" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>