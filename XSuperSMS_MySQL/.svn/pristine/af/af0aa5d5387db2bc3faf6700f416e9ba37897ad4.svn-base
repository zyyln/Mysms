    <!-- 导航画面 -->
    <dimen name="width_navi">80dp</dimen>

    <!-- 入库 -->
    <dimen name="width_selecthouse">120dp</dimen>
    <dimen name="height_selcthouse">25dp</dimen>
    <dimen name="GridSpacing">9pt</dimen>

    <!-- 行车弹出框 -->
    <dimen name="height_crane">240dp</dimen>

    <!-- 系统设定 -->
    <dimen name="height_changePsd">45dp</dimen>

