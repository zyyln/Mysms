    <!-- ************源文件************** -->
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>

    <!-- ********设置应用字体(单位sp)******** -->
    <!-- 常规字体大小级别 -->
    <dimen name="textSize_5">8sp</dimen>
    <dimen name="textSize_4">10sp</dimen>
    <dimen name="textSize_3">12sp</dimen>
    <dimen name="textSize_2">14sp</dimen>
    <dimen name="textSize_1">20sp</dimen>
    <!-- 特殊字体大小 -->
    <dimen name="textSize_large">40sp</dimen>
    <dimen name="textSize_normal">30sp</dimen>
    <dimen name="textSize_small">28sp</dimen>

    <!-- *******设置应用距离尺寸(单位dp)******* -->
    <!-- 顶部条的高度 -->
    <dimen name="height_bar_top">30dp</dimen>
    <!-- 底部条的高度 -->
