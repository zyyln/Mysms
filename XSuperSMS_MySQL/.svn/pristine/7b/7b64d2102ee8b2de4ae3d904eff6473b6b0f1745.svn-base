<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/activity_bg"
    android:orientation="vertical" >

    <include
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_bar_top"
        layout="@layout/layout_header" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="5dp"
        android:layout_weight="1"
        android:background="@drawable/plan_content_bj"
        android:orientation="vertical" >

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/gray"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="@string/contact_name" />

                <TextView
                    android:id="@+id/textView1"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:paddingLeft="16dp"
                    android:text="@string/contact_cropation" />

                <TextView
                    android:id="@+id/textView2"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/gray"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="@string/contact_info_title" />

                <TextView
                    android:id="@+id/textView3"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:paddingLeft="16dp"
                    android:text="@string/contact_infot_text" />

                <TextView
                    android:id="@+id/textView4"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/gray"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="@string/contact_tel_title" />

                <TextView
                    android:id="@+id/textView5"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:paddingLeft="16dp"
                    android:text="@string/contact_tel_text" />

                <TextView
                    android:id="@+id/textView6"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/gray"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="@string/email" />

                <TextView
                    android:id="@+id/textView7"
                    style="@style/font2_black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:paddingLeft="16dp"
                    android:text="@string/contact_email_text" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <include
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_bar_bottom"
        layout="@layout/layout_footer" />

</LinearLayout>